<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:FlyoutClickThrough"
            x:Class="FlyoutClickThrough.MainPage">
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:DetailPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage />
    </FlyoutPage.Flyout>
</FlyoutPage>
